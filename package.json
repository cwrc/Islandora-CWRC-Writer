{
  "name": "islandora-cwrc-writer",
  "version": "0.9.0",
  "description": "A wrapper to use CWRC-Writer with Islandora.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwrc/Islandora-CWRC-Writer.git"
  },
  "keywords": [
    "cwrc",
    "islandora",
    "drupal"
  ],
  "author": "Andrew MacDonald",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/cwrc/Islandora-CWRC-Writer/issues"
  },
  "homepage": "https://github.com/cwrc/Islandora-CWRC-Writer#readme",
  "scripts": {
    "build": "npm run build-cwrc-writer-base && npm run webpack-prod",
    "webpack-prod": "webpack --config webpack.prod.js",
    "webpack-dev": "webpack --config webpack.dev.js --debug=true --display-error-details=true",
    "webpack-dev-watch": "webpack --config webpack.dev.js --debug=true --display-error-details=true --watch",
    "dev-server": "webpack-dev-server --config webpack.dev.js",
    "build-cwrc-writer-base": "cd \"node_modules/cwrc-writer-base\" && npm run build && npm run build-scoped-css -- --global-var=\"parentId=#cwrc_wrapper\"",
    "cwrc-writer-css-dev": "cd \"node_modules/cwrc-writer-base\" && npm run build-css",
    "stats": "webpack --profile --json > build/stats.json",
    "analyzer": "webpack-bundle-analyzer build/stats.json"
  },
  "dependencies": {
    "cwrc-git-dialogs": "1.17.0",
    "cwrc-public-entity-dialogs": "1.16.4",
    "cwrc-tei-entities-lookup": "^1.3.0",
    "cwrc-writer-base": "4.0.0",
    "dbpedia-entity-lookup": "^0.6.1",
    "geonames-entity-lookup": "^1.2.0",
    "getty-entity-lookup": "^1.5.0",
    "lgpn-entity-lookup": "^1.0.0",
    "viaf-entity-lookup": "^1.4.1",
    "wikidata-entity-lookup": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babelify": "^10.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "copyfiles": "^2.1.1",
    "core-js": "^2.6.9",
    "eslint": "^6.6.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "mkdirp": "^0.5.1",
    "path-exists-cli": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
