{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jquery": true,
        "drupal": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "require-atomic-updates": "off",
        "quotes": [2, "single"]
    }
}